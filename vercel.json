{
  "redirects": [
    {
      "source": "/(.*)",  
      "has": [
        {
          "type": "host",
          "value": "diademfoods.com"  
        }
      ],
      "destination": "https://www.diademfoods.com/$1",  
      "permanent": true   
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
